{
  "name": "variojs-toolbox",
  "version": "1.0.0-beta.2",
  "description": "Create/maintain variojs (parallax)animations",
  "main": "./dist",
  "scripts": {
    "watch": "NODE_ENV=development webpack --config webpack-lib.config.js --watch",
    "build-lib": "NODE_ENV=production webpack --mode production --config webpack-lib.config.js",
    "webpack-dev-server": "NODE_ENV=development webpack-dev-server --mode development --config webpack.config.js",
    "start": "electron ./main-dev.js",
    "test": "echo \"Error: no test specified\" && exit 1",
    "check-types": "tsc",
    "pack": "NODE_ENV=production webpack --mode production && electron-builder --dir",
    "dist": "NODE_ENV=production webpack --mode production && electron-builder"
  },
  "build": {
    "appId": "variojsapp",
    "dmg": {
      "contents": [
        {
          "x": 110,
          "y": 150
        },
        {
          "x": 240,
          "y": 150,
          "type": "link",
          "path": "/Applications"
        }
      ]
    }
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/jk-wd/variojs-toolbox.git"
  },
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/jk-wd/variojs-toolbox/issues"
  },
  "homepage": "https://github.com/jk-wd/variojs-toolbox#readme",
  "dependencies": {
    "@types/react": "^16.9.34",
    "@types/react-dom": "^16.9.7",
    "@types/styled-components": "^5.1.0",
    "babel-plugin-module-resolver": "^4.0.0",
    "babel-polyfill": "^6.26.0",
    "express": "^4.17.1",
    "open-cli": "^6.0.1",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "semver": "^7.3.2",
    "shelljs": "^0.8.4",
    "source-map-loader": "^0.2.4",
    "styled-components": "^5.1.0",
    "variojs": "^1.0.0-beta.1",
    "ws": "^7.3.0"
  },
  "devDependencies": {
    "@babel/core": "^7.9.6",
    "@babel/plugin-proposal-class-properties": "^7.8.3",
    "@babel/plugin-proposal-object-rest-spread": "^7.9.6",
    "@babel/preset-env": "^7.9.6",
    "@babel/preset-react": "^7.9.4",
    "@babel/preset-typescript": "^7.9.0",
    "babel-loader": "^8.1.0",
    "electron": "^9.0.5",
    "electron-builder": "^22.7.0",
    "fork-ts-checker-webpack-plugin": "^4.1.5",
    "install-peers-cli": "^2.2.0",
    "script-launcher": "^1.29.0",
    "typescript": "^3.8.3",
    "webpack": "^4.43.0",
    "webpack-cli": "^3.3.11",
    "webpack-dev-server": "^3.11.0"
  }
}
